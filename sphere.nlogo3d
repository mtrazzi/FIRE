to update-u
  ;; TODO: Use an M parameter
  ;; New up must be modified by a delta-mu chosen in the range [-M, +M]
  if typ = 0 [
    if random-float 1 < puc [
      set up ((random-float 5) + 5)
      if up > 10 [set up 10]
      if up < 5 [set up 5] ] ]
  if typ = 1 [
    if random-float 1 < puc [
      set up (random-float 5)
      if up > 5 [set up 5]
      if up < 0 [set up 0] ] ]
  if typ = 3 [
