to-report perform
  let res 0
  ifelse typ = 2 [
    set res (random-float (PL-GOOD - PL-BAD)) + PL-BAD
  ]
  [
    set res random-normal up sigmap
  ]
  ;; Clamp value between min and max
  if res > 10 [ set res 10 ]
  if res < -10 [ set res -10 ]

  report res
end

